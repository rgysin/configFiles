[user]
	email = rgysin@umich.edu
	name = Ryan Gysin
[push]
    default = simple
[alias]
    logchart = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
[core]
	editor = vim
[filter "spaces2tabs"]
    clean = unexpand --tabs=4 --first-only
    smudge = unexpand --tabs=4 --first-only
